    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-prettier": "^2.3.0",
    "eslint": "^4.7.0",
    "jest": "^20.0.4",
